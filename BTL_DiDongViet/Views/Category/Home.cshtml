                    </div>
                </div>
                <div class="banner__right-wrapper">
                    <div class="banner__right-banner1"></div>
                    <div class="banner__right-banner2"></div>
                </div>
            </div>
            <div class="category-list">
                @for (int i = 0; i < 10; i++)
                {
                    <a href="@Url.Action("Products", "Product", new { @id = Model.Product.ElementAt(i).ID })" >
                        <div style="width: 100px" class="icon-hover">
                            <div class="icon-wrapper">
                                <div>
