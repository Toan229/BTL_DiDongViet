                    @if (IPProduct.Count != 0)
                    {
                        foreach (var item in IPProduct)
                        {
                            count++;
                            <a href="#" class="product-list_item">
                                @{
                                    var price = item.Price;
                                    int sale = (int)(price - price * item.Sale / 100);
                                }
                                <div class="item__img" style="background-image: url(../../assets/img/ProductImages/@item.Image);"></div>
                                <div class="item__text" style="font-size:12px;color:black;">
                                    @item.ProductName
                                </div>
                                <div class="item__text" style="text-decoration: line-through; font-size: 14px;">
                                    Giá @String.Format(moneyFomat, price) đ
                                </div>
                                <div class="item__text">
                                    Còn  @String.Format(moneyFomat, sale) đ
                                </div>
                                <div class="discount-badge">
                                    <div class="discount-badge-text">
                                        <span class="percentage">Giảm<br />@item.Sale.ToString()%</span>
                                    </div>
                                </div>
                            </a>
                            if(count == 3)
                            {
                                break;
                            }
                        }
                    }

                </div>
                <div class="middle-space" style="background-color: #ffebda;
                                             display: flex;">
                    <span style="width: 13px;
                                                height: 20px;"></span>
                </div>
                <div class="product-list__right">
