            <header>
                @{
                    int count = 0;
                    var moneyFomat = "{0:#,00}";
                    var IPCategory = Model.Category.Find(m => m.Name.Equals("Apple Iphone"));
                    var IPProduct = Model.Product.FindAll(p => p.CategoryID == IPCategory.ID).OrderByDescending(p => p.Sale).ToList();
                    var setsCategory = Model.Category.Find(s => s.Name.Equals("Phụ kiện"));
                    var sets = Model.Product.FindAll(p => p.CategoryID == setsCategory.ID).OrderBy(p => p.Price).ToList();
                }
                <div class="header-left">
                    <span class="header__title">Iphone sale mạnh</span>
                    <a class="header__link" href="@Url.Action("Category", "Category", new { @name = IPCategory.Name })">
                        Xem thêm
                        <svg enable-background="new 0 0 13 21" viewBox="0 0 13 21" style="width: 9px;
                                 height: 9px;
                                 overflow: hidden;">
                            <path stroke="none" d="m11.1 9.9l-9-9-2.2 2.2 8 7.9-8 7.9 2.2 2.1 9-9
                                    1-1z"></path>
                        </svg>
                    </a>
                </div>
                <div class="header-right">
                    <span class="header__title">Phụ kiện giá sốc</span>
                    <a class="header__link" href="@Url.Action("Category", "Category", new { @name = setsCategory.Name }) ">
                        Xem thêm
                        <svg enable-background="new 0 0 13 21" viewBox="0 0 13 21" style="width: 9px;
                                 height: 9px;
                                 overflow: hidden;">
                            <path stroke="none" d="m11.1 9.9l-9-9-2.2 2.2 8 7.9-8 7.9 2.2 2.1 9-9
                                    1-1z"></path>
                        </svg>
                    </a>
                </div>
            </header>
            <div class="product-list">
