    <div class="section-banner-hotword">
        <div class="banner-wrapper">
            <div class="banner">
                <div>
                    <div class="trend-body">
                        <div class="trend-item">
                            <span class="text-trend">#1</span>
                            <span class="text-trend">Ten San Pham</span>
                            <div class="trend-desc">Có hàng ngay, không cần đợi</div>
                        </div>
                        <div class="trend-item">
                            <span class="text-trend">#2</span>
                            <span class="text-trend">Ten San Pham</span>
                            <div class="trend-desc">Nhỏ gọn. Nhiều ưu đãi</div>
                        </div>
                        <div class="trend-item">
                            <span class="text-trend">#3</span>
                            <span class="text-trend">Ten San Pham</span>
                            <div class="trend-desc">Mẫu mã đẹp. Giá tốt</div>
                        </div>
                        <div class="trend-item">
                            <span class="text-trend">#4</span>
                            <span class="text-trend">Ten San Pham</span>
                            <div class="trend-desc">Nhiều mẫu. Giá hấp dẫn</div>
                        </div>
                        <div class="trend-item">
                            <span class="text-trend">#5</span>
                            <span class="text-trend">Ten San Pham</span>
                            <div class="trend-desc">Nhỏ gọn. Định vị thông minh</div>
                        </div>
                    </div>
                </div>
                <div class="banner__main">
                    <div class="stardust-carouse__arrow">
                        <div class="stardust-carouse__arrow--prev">
                            <svg>
                                <path stroke="none" d="m4.2 10l7.9-7.9-2.1-2.2-9 9-1.1 1.1 1.1 1 9 9
                                       2.1-2.1z"></path>
                            </svg>
                        </div>
                        <div class="stardust-carouse__arrow--next">
                            <svg>
                                <path stroke="none" d="m11.1 9.9l-9-9-2.2 2.2 8 7.9-8 7.9 2.2 2.1
                                       9-9 1-1z"></path>
                            </svg>
